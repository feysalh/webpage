[
    {
      "type": 1,
      "title": "Openeyes",
      "description": "Docker for Openeyes",
      "categories": ["docker"],
      "platform": "linux",
      "logo": "",
      "image": "appertaopeneyes/web-allin1:latest",
      "ports": [
        "80/tcp",
        "3306/tcp"
      ],
        "env": [
          {
            "name": "ENV_VAR_WITH_DEFAULT_VALUE",
            "default": "default_value",
            "preset":  true
          },
          {
            "name": "ENV_VAR_WITH_SELECT_VALUE",
            "label": "An environment variable",
            "description": "A description for this env var",
            "select": [
              {
            "text": "UID",
            "value": "1",
            "default": true
          },
          {
            "text": "GID",
            "value": "1",
            "preset": true
          },
          {
            "text": "OE_MODE",
            "value": "DEV",
            "preset": true
          },
          {
            "text": "GIT_USER",
            "value": "feysalh"
           },
           {
            "text": "PATH",
            "value": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
            },
            {
            "text": "PHP_VERSION",
            "value": "5.6"
            },
            {
            "text": "WROOT",
            "value": "/var/www/openeyes",
             },
             {
            "text": "LOCAL_DB",
            "value": "TRUE",
            "description": "A description for this env var"
             },
             {
            "name": "BUILD_BRANCH",
            "value": "master"
              },
              {
            "name": "DOCKER_CONTAINER",
            "value": "TRUE"
             },
             {
            "name": "DATABASE_HOST",
            "value": "TRUE"
             },
             {
            "name": "DATABASE_HOST",
            "value": "localhost"
             },
             {
            "name": "DATABASE_NAME",
            "value": "openeyes"
             },
             {
            "name": "DATABASE_PORT",
            "value": "3306"
              },
              {
            "name": "DATABASE_USER",
            "value": "openeyes"
              },
              {
            "name": "DATABASE_PASS",
            "value": "openeyes"
             },
             {
            "name": "USE_DEMO_DATA",
            "value": "TRUE"
              },
              {
            "name": "GIT_ORG",
            "value": ""
              },
              {
            "name": "MYSQL_ROOT_PASSWORD",
            "value": ""
            },
            {
            "name": "MYSQL_SUPER_USER",
            "default": "root",      
             },
             {
             "labels": [
                { "name": "com.docker.compose.config-hash", "value": "ea719b5eeead79c07fa20ad5ae3e98debd78975ce8ffafbbdb543bc5cd2acdb5" },
                { "name": "com.docker.compose.container-number", "value": "1" },
                { "name": "com.docker.compose.oneoff", "value": "False" },
                { "name": "com.docker.compose.oneoff", "value": "feysalh" },
                { "name": "com.docker.compose.oneoff", "value": "openeyes" },
                { "name": "com.docker.compose.oneoff", "value": "1.23.2" }
              ]
            },
            {
            "name": "com.docker.compose.oneoff",
            "default": "False",
            "description": "A description for this env var"
             },
             {  
            "name": "com.docker.compose.project",
            "default": "feysalh",
            "description": "A description for this env var"
             },
             {
            "name": "com.docker.compose.service",
            "default": "openeyes",
            "description": "A description for this env var"
            },
            {
            "name": "com.docker.compose.version",
            "default": "1.23.2",
            "description": "A description for this env var"
            },
            {
              "volumes": [
              {
                "container": "/var/lib/mysql"
              },
              {
                "container": "/var/www/openeyes"
              },
              {
                "container": "/run/secrets/SSH_PRIVATE_KEY",
                "bind": "/Users/feysalh/.ssh/id_rsa",
                "readonly": true
              }
            ]
          }
]
}
]
